<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar fx:id="menu" prefHeight="25.0" prefWidth="870.0" VBox.vgrow="NEVER">
      <menus>
            <Menu fx:id="profile" mnemonicParsing="false" text="Profile">
              <items>
                <MenuItem fx:id="viewProfile" mnemonicParsing="false" text="View my profile" />
                  <MenuItem fx:id="updateProfile" mnemonicParsing="false" text="Update my profile" />
              </items>
            </Menu>
      </menus>
    </MenuBar>
      <Label fx:id="welcomelabel" prefHeight="32.0" prefWidth="597.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin></Label>
      <VBox prefHeight="382.0" prefWidth="600.0">
         <children>
            <TableView fx:id="eventTable">
              <columns>
                <TableColumn fx:id="eventName" prefWidth="75.0" text="EVENT" />
                <TableColumn fx:id="venue" prefWidth="75.0" text="VENUE" />
                  <TableColumn fx:id="day" prefWidth="75.0" text="DAY" />
                  <TableColumn fx:id="price" prefWidth="75.0" text="PRICE" />
                  <TableColumn fx:id="soldTickets" prefWidth="75.0" text="SOLD" />
                  <TableColumn fx:id="totalTickets" prefWidth="113.0" text="TOTAL" />
                  <TableColumn fx:id="availableTickets" prefWidth="83.5" text="AVAILABLE" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TableView>
         </children></VBox>
  </children>
</VBox>
