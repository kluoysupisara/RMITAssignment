<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="450.0" prefWidth="544.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar fx:id="menu" prefHeight="25.0" prefWidth="870.0" VBox.vgrow="NEVER">
      <menus>
            <Menu fx:id="profile" mnemonicParsing="false" text="Profile">
              <items>
                <MenuItem fx:id="viewProfile" mnemonicParsing="false" text="View my profile" />
                  <MenuItem fx:id="updateProfile" mnemonicParsing="false" text="Update my profile" />
              </items>
            </Menu>
      </menus>
    </MenuBar>
      <Label fx:id="welcomelabel" />
      <TableView fx:id="eventTable" prefHeight="619.0" prefWidth="544.0">
        <columns>
          <TableColumn fx:id="eventName" prefWidth="75.0" text="EVENT" />
          <TableColumn fx:id="venue" prefWidth="75.0" text="VENUE" />
            <TableColumn fx:id="day" prefWidth="75.0" text="DAY" />
            <TableColumn fx:id="price" prefWidth="75.0" text="PRICE" />
            <TableColumn fx:id="soldTickets" prefWidth="75.0" text="SOLD TICKETS" />
            <TableColumn fx:id="totalTickets" prefWidth="75.0" text="TOTAL TICKETS" />
            <TableColumn fx:id="availableTickets" prefWidth="92.0" text="AVAILABLE TICKET" />
            <TableColumn fx:id="actionColumn" prefWidth="92.0" text="BOOKING" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox prefHeight="200.0" prefWidth="100.0" />
  </children>
</VBox>
